    email=andrewrkowalczyk@gmail.com

[core]
    pager="diff-so-fancy | less --tabs=4 -RFX"

[color]
    ui=true

[diff-highlight]
    oldNormal="red bold"
    oldHighlight="red bold 52"
    newNormal="green bold"
    newHighlight="green bold 22"

[diff]
    meta="yellow"
    frag="magenta bold"
    commit="yellow bold"
    old="red bold"
    new="green bold"
    whitespace="red reverse"
